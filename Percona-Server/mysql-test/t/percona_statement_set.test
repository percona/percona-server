--echo '# During Execution values
SET STATEMENT myisam_sort_buffer_size=400000,
              myisam_repair_threads=2,
              sort_buffer_size=200000,
              binlog_format=row,
              keep_files_on_create=OFF,
              max_join_size=4444440000000 FOR
              SELECT myProc(123.45);
--echo ''
