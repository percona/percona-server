SET STATEMENT max_join_size = 0 FOR DROP TABLE t2;
ERROR HY000: Lock wait timeout exceeded; try restarting transaction
UNLOCK TABLES;
COMMIT;
SET SESSION lock_wait_timeout = @old_lock_wait_timeout;
