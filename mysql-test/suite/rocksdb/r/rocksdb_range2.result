part_id smallint unsigned NOT NULL,
oid bigint unsigned NOT NULL,
tid bigint unsigned NOT NULL,
filler char(32),
KEY tid (part_id,tid,oid)
) ENGINE=ROCKSDB;
set session transaction isolation level read committed;
