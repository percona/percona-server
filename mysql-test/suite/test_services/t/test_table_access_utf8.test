# Test the table_access_service

SET NAMES "utf8mb4";

INSTALL COMPONENT 'file://component_test_table_access';

# MATH

# Schema name
# \u2200 = for each
# \u220A = element
# \u2119 = P

# Table name
# \u2203 = there exists
# \u220A = element
# \u211A = Q (UTF8MB3)
# \u1D54A = S (UTF8MB4)

# Column name
# \u224E = equivalent
# \u22a4 = Truth

SELECT HEX('∀p∊ℙ');
SELECT HEX('∃s∊ℚ');
SELECT HEX('∃s∊𝕊');
SELECT HEX('s(p)≎⊤');

CREATE SCHEMA `∀p∊ℙ`;

show databases;

CREATE TABLE `∀p∊ℙ`.`∃s∊ℚ`(`s(p)≎⊤` VARCHAR(255) CHARACTER SET `utf8mb4`);

CREATE TABLE `∀p∊ℙ`.`∃s∊𝕊`(`s(p)≎⊤` VARCHAR(255) CHARACTER SET `utf8mb4`);

use `∀p∊ℙ`;

show tables;

show create table `∃s∊ℚ`;

show create table `∃s∊𝕊`;

SELECT * FROM `∀p∊ℙ`.`∃s∊ℚ`;

SELECT * FROM `∀p∊ℙ`.`∃s∊𝕊`;

SELECT test_table_access_driver("MATH-INSERT-UTF8MB3");

SELECT * FROM `∀p∊ℙ`.`∃s∊ℚ`;
TRUNCATE TABLE `∀p∊ℙ`.`∃s∊ℚ`;

SELECT * FROM `∀p∊ℙ`.`∃s∊ℚ`;

SELECT test_table_access_driver("MATH-INSERT-UTF8MB4");

SELECT * FROM `∀p∊ℙ`.`∃s∊𝕊`;
TRUNCATE TABLE `∀p∊ℙ`.`∃s∊𝕊`;

SELECT * FROM `∀p∊ℙ`.`∃s∊𝕊`;

--echo ##################
--echo # SERVER RESTART #
--echo ##################

--source include/restart_mysqld.inc

SELECT * FROM `∀p∊ℙ`.`∃s∊ℚ`;

SELECT * FROM `∀p∊ℙ`.`∃s∊𝕊`;

DROP TABLE `∀p∊ℙ`.`∃s∊ℚ`;
DROP TABLE `∀p∊ℙ`.`∃s∊𝕊`;
DROP SCHEMA `∀p∊ℙ`;

UNINSTALL COMPONENT 'file://component_test_table_access';

